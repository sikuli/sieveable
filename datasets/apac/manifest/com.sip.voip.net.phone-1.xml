<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.sip.voip.net.phone" versionName="1.0" versionCode="1">
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.USE_SIP"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="android.permission.RECORD_AUDIO"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.WRITE_CONTACTS"/>
    <uses-permission android:name="android.permission.VIBRATE"/>
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS"/>
    <uses-feature android:name="android.hardware.sip.voip" android:required="true"/>
    <uses-feature android:name="android.hardware.wifi" android:required="true"/>
    <uses-feature android:name="android.hardware.microphone" android:required="true"/>
    <application android:debuggable="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@style/AppTheme">
        <receiver android:label="Call Receiver" android:name="com.sip.voip.net.phone.IncomingCallReceiver"/>
        <activity android:label="@string/title_activity_main" android:name="com.sip.voip.net.phone.Phone">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="SIP Account Settings - Net Phone" android:name="com.sip.voip.net.Preferences.AccountSettings"/>
    </application>
<uses-sdk android:targetSdkVersion="15" android:minSdkVersion="15" android:maxSdkVersion="15"/></manifest>
